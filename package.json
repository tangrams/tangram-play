    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "gm": "1.23.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-notify": "3.0.0",
    "gulp-postcss": "6.4.0",
    "gulp-sourcemaps": "latest",
